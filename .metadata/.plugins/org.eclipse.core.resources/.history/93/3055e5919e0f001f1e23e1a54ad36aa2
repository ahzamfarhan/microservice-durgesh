}

public class Test {
	
	
	public static void main(String[] a) {
		
		List<Integer> list = new ArrayList<Integer>();
		
		list.add(new Integer(10));
		list.add(new Integer(11));
		list.add(new Integer(13));
		list.add(new Integer(14));
